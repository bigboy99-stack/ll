{
  "private": true,
  "name": "projectone",
  "version": "1.0.0",
  "description": "",

  "scripts": {
    "minify:js": "for %f in (src\\prices.js) do (terser \"%f\" -o \"dist/%~nf.js\" -c -m)",
    "minify:css": "for %f in (src\\prices.css) do (postcss -o \"dist/%~nf.css\" \"%f\")",
    "minify:html": "for %f in (src\\*.html) do (html-minifier-terser --collapse-whitespace --remove-comments \"%f\" -o \"dist/%~nf.html\")",
    "bes:js": "for %f in (src\\bespokeJoinery\\*.js) do (terser \"%f\" -o \"dist/bespokeJoinery/%~nf.js\" -c -m)",
    "bes:css": "for %f in (src\\bespokeJoinery\\*.css) do (postcss -o \"dist/bespokeJoinery/%~nf.css\" \"%f\")",
    "bes:html": "for %f in (src\\bespokeJoinery\\*.html) do (html-minifier-terser --collapse-whitespace --remove-comments \"%f\" -o \"dist/bespokeJoinery/%~nf.html\")",
    "scaff:js": "for %f in (src\\scaffoldingHire\\*.js) do (terser \"%f\" -o \"dist/scaffoldingHire/%~nf.js\" -c -m)",
    "scaff:css": "for %f in (src\\scaffoldingHire\\*.css) do (postcss -o \"dist/scaffoldingHire/%~nf.css\" \"%f\")",
    "scaff:html": "for %f in (src\\scaffoldingHire\\*.html) do (html-minifier-terser --collapse-whitespace --remove-comments \"%f\" -o \"dist/scaffoldingHire/%~nf.html\")",
    "build": "npm run minify:js & npm run minify:css & npm run minify:html & npm run bes:js & npm run bes:css & npm run bes:html",
    "scaff": "npm run scaff:js & npm run scaff:css & npm run scaff:html"
  },

  "keywords": [],
